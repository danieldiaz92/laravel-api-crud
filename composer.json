{
    "name": "sofis/crud-generator",
    "description": "Laravel Crud Generator",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/danieldiaz92/laravel-api-crud/releases/tag/v1.0.2"
        }
    ],
    "authors": [
        {
            "name": "M Awais",
            "email": "asargodha@gmail.com"
        }
    ],
    "keywords": ["laravel", "crud", "crud generator", "laravel crud generator", "laravel package"],
    "require": {
        "laravel/framework": "^5.5|^6.20.42|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sofis\\CrudGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sofis\\CrudGenerator\\CrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
