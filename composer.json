{
    "name": "sofis/crud-generator",
    "description": "Laravel Crud Generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Diaz",
            "email": "daniel.diaz@sofis.lat"
        }
    ],
    "keywords": ["laravel", "crud", "crud generator", "laravel crud generator", "laravel package"],
    "require": {
        "laravel/framework": "^10.0"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Sofis\\CrudGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sofis\\CrudGenerator\\CrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "repositories": {
        "packagist.org": false,
        "private-packagist": {
            "type": "composer",
            "url": "https://repo.packagist.com/daniel/"
        }
    }
}
